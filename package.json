{
  "name": "ss.js",
  "version": "1.0.0",
  "description": "A single store state managment for react hooks.",
  "main": "index.js",
  "module": "dist/ss.umd.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=prod webpack --config webpack.config.js",
    "bundle": "cross-env NODE_ENV=bundle webpack --profile --json > state.json --config webpack.config.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryouaki/ss.js.git"
  },
  "keywords": [
    "react",
    "redux",
    "hooks",
    "state"
  ],
  "author": "ryouaki (46517115@qq.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ryouaki/ss.js/issues"
  },
  "homepage": "https://github.com/ryouaki/ss.js#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
